class Solution {
    public boolean isIsomorphic(String s, String t) {
        HashSet<Character> hashSet1 = new HashSet<Character>();
        HashSet<Character> hashSet2 = new HashSet<Character>();
        for(int i = 0;i<s.length();i++){
            hashSet1.add(s.charAt(i));
            hashSet2.add(t.charAt(i));
        }
        if(hashSet1.size()==hashSet2.size()){
            return true;
        }
        return false;
        
    }
}


// Time Complexity : O(n)
// On leetcode 32/39 testcases passed
